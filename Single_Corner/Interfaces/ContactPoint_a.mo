within ABS.Single_Corner.Interfaces;

connector ContactPoint_a "Contact point belonging to the rolling BODY"
  extends ABS.Single_Corner.Interfaces.ContactPoint;
  annotation(
    Icon(graphics = {Rectangle(origin = {-20, 0}, fillColor = {0, 127, 0}, fillPattern = FillPattern.Solid, extent = {{-80, 40}, {80, -40}}), Line(origin = {50, -30}, points = {{10, -10}, {-2, 2}}), Line(origin = {-90, -30}, points = {{10, -10}, {-2, 2}}), Line(origin = {-70, -30}, points = {{10, -10}, {-2, 2}}), Line(origin = {-50, -30}, points = {{10, -10}, {-2, 2}}), Line(origin = {-30, -30}, points = {{10, -10}, {-2, 2}}), Line(origin = {-10, -30}, points = {{10, -10}, {-2, 2}}), Line(origin = {10, -30}, points = {{10, -10}, {-2, 2}}), Line(origin = {30, -30}, points = {{10, -10}, {-2, 2}})}),
    Diagram(graphics = {Text(origin = {-20, 10}, extent = {{-80, 10}, {80, -10}}, textString = "%name"), Rectangle(origin = {-20, -20}, fillColor = {0, 127, 0}, fillPattern = FillPattern.Solid, extent = {{-80, 20}, {80, -20}}), Line(origin = {0.55, -100.63}, points = {{-100, 0}, {100, 0}}, thickness = 0.75, arrow = {Arrow.None, Arrow.Filled}, arrowSize = 20)}),
    Documentation(info = "<html><head></head><body><p style=\"font-size: 12px;\"><span style=\"font-family: 'DejaVu Sans Mono';\">This is a connector for a 1D contact&nbsp;</span><span style=\"font-family: 'DejaVu Sans Mono';\">point belonging to the rolling <b>body</b>. The force vector f and the slipping velocity vector share the same unit vector, i.e. when both are positive they have the same direction and sign.</span></p><div class=\"htmlDoc\" style=\"font-family: 'DejaVu Sans Mono';\"><p style=\"font-size: 12px;\">The following variables are defined in this connector:</p><blockquote style=\"font-size: 12px;\"><p>slip:&nbsp;<b>Normalized</b>&nbsp;slipping velocity of the contact point [adim].</p><p>f: Friction force with the <b>same</b> positive axis as the slipping velocity [adim].</p></blockquote></div><div class=\"textDoc\"><p style=\"font-family: 'DejaVu Sans Mono'; font-size: 12px;\"></p></div></body></html>"));
end ContactPoint_a;